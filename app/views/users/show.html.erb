<h1><%= @user.name %></h1>
<%= image_tag @user.avatar(:thumb) %>
<div class="row">
  <div class="col-md-6">
    <div id="my-tweets">
      <h3>My Tweets</h3>
      <% @tweets.each do |tweet| %>
        <div class="tweet-display">
          <%= tweet.message %>
        </div>
      <% end %>
    </div>
  </div>

  <div class="col-md-6">
    <h3>My Feed</h3>
  </div>
</div>


